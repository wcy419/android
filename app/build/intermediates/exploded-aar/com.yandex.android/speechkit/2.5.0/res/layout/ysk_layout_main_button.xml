<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/ysk_default_divider_height"
        android:background="#F2F2F2" />

    <TextView
        android:id="@+id/main_btn"
        android:layout_width="match_parent"
        android:layout_height="@dimen/ysk_content_main_button_height"
        android:background="@drawable/ysk_background_selector"
        android:gravity="center"
        android:textColor="#262626"
        android:textSize="@dimen/ysk_content_text_size" />

</LinearLayout>
